{
  "schemaVersion": 1,
  "id": "fabricdrawer",
  "version": "${version}",
  "name": "Fabric Drawer",
  "description": "Fabric library mod for kotlin mods that allows you to easily save data to NBT and PacketByteBuf using kotlinx.serialization.\n",
  "authors": [
    "Fudge"
  ],
  "contact": {
    "homepage": "https://github.com/natanfudge/Fabric-Drawer",
    "sources": "https://github.com/natanfudge/Fabric-Drawer"
  },
  "license": "The MIT License",
  "environment": "*",
  "mixins": [
    "fabric-drawer.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.0",
    "fabric-language-kotlin": ">=1.8.3"
  },
  "custom": {
    "modmenu:api": true
  }
}
